{
  "name": "6q-boilerplate-hatenablog_theme",
  "version": "1.0.0",
  "description": "Boilerplate for Hatena Blog theme by Rokuzeudon",
  "author": "Rokuzeudon",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all scss autoprefixer",
    "watch": "chokidar \"scss/\" -c \"npm run build\"",
    "scss": "node-sass scss/style.scss build/style.css --output-style compressed",
    "autoprefixer": "postcss --use autoprefixer -r build/style.css --no-map"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "chokidar-cli": "^1.2.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^4.1.1"
  }
}
